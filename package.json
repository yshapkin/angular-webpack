    "node": ">=6.9.0"
  },
  "scripts": {
    "lint": "tslint 'src/client/app/**/*.ts' --force",
    "lint:fix": "tslint 'src/client/app/**/*.ts' --fix --force",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "ngc": "ngc -p tsconfig-aot.json",
    "cleanup": "rimraf build/",
    "start": "npm run webpack:dev",
    "webpack:build": "npm run ngc && webpack --config webpack/webpack.vendor.js && webpack --config webpack/webpack.dev.js",
    "webpack:build:dev": "webpack --config webpack/webpack.dev.js",
    "webpack:build:vendor": "webpack --config webpack/webpack.vendor.js",
    "webpack:dev": "webpack-dev-server --config webpack/webpack.dev.js  --progress --inline --hot --profile --port=9060",
    "webpack:prod": "npm run ngc && npm run test && webpack -p --config webpack/webpack.vendor.js && webpack -p --config webpack/webpack.prod.js",
    "test": "npm run lint && karma start src/test/karma.conf.js",
    "test:watch": "karma start --watch",
    "e2e": "protractor src/test/protractor.conf.js",
    "postinstall": "webdriver-manager update && npm run webpack:build"
  }
}
